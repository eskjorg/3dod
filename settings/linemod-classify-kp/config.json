{
    "network": {
        "encoder": "resnet34",
        "output_stride": 4,
        "support_region": 1.0
    },
    "logging": {
        "save_kitti_format": false
    },
    "training": {
        "learning_rate": 0.0003,
        "nll_loss": true
    },
    "visualization": {
        "gt": [
            "bbox2d",
            "keypoints"
        ],
        "det": [
        ]
    },
    "postprocessing": {
        "methods": [
        ]
    },
    "data":{
        "dataformat": "sixd_kp_instances",
        "path": "/datasets/occluded-linemod-augmented",
        "sequences": {
            "train": [
                "train_aug/ape",
                "train_aug/can",
                "train_aug/cat",
                "train_aug/driller",
                "train_aug/duck",
                "train_aug/eggbox",
                "train_aug/glue",
                "train_aug/holepuncher"
            ],
            "val": [
                "train_occl/ape"
            ],
            "test": [
                "test_occl/benchviseblue"
            ]
        },
        "img_dims": [480, 640]
    },
    "loading": {
        "train": {
            "batch_size": 8,
            "shuffle": true,
            "num_workers": 6
        },
        "val": {
            "batch_size": 4,
            "shuffle": false,
            "num_workers": 6
        },
        "test": {
            "batch_size": 1,
            "shuffle": false,
            "num_workers": 6
        }
    }
}
