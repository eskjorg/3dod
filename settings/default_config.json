{
    "network": {
        "encoder": "resnet34",
        "tiling_upsampling": 8,
        "output_stride": 4,
        "support_region": 0.2
    },
    "validation": {
        "score": "f1_score"
    },
    "training": {
        "learning_rate": 0.0001,
        "n_epochs": 100
    },
    "loading": {
        "train": {
            "batch_size": 16,
            "shuffle": true,
            "num_workers": 4
        },
        "val": {
            "batch_size": 16,
            "shuffle": false,
            "num_workers": 4
        },
        "test": {
            "batch_size": 1,
            "shuffle": false,
            "num_workers": 4
        }
    }
}
